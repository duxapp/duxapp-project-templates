{
  "name": "duxappReactNative",
  "description": "react-native基础系统",
  "version": "1.0.17",
  "dependencies": [
    "duxapp"
  ],
  "npm": {
    "scripts": {
      "build:rn-android": "duxapp runtime enterFile && taro build --type rn --platform android",
      "build:rn-ios": "duxapp runtime enterFile && taro build --type rn --platform ios",
      "android": "react-native run-android",
      "debug:android": "duxapp runtime enterFile && duxapp rn create && cd android && ./gradlew installDebug && cd ..",
      "build:android": "duxapp runtime enterFile && duxapp rn create && cd android && ./gradlew assembleRelease && cd .. && duxapp file copy android/app/build/outputs/apk/release/app-arm64-v8a-release.apk dist/build/app-arm64-v8a-release.apk",
      "pod": "pod-install",
      "ios": "duxapp runtime enterFile && duxapp rn create && pod-install && react-native run-ios && cd .",
      "build:ios": "duxapp ios build",
      "export:ios": "duxapp ios export",
      "upload-ios": "duxapp ios upload dist/ios/duxapp.ipa/duxapp.ipa"
    },
    "dependencies": {
      "@bam.tech/react-native-image-resizer": "^3.0.5",
