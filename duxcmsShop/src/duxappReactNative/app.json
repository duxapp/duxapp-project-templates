      "expo-barcode-scanner": "~12.3.2",
      "expo-brightness": "~11.2.1",
      "expo-camera": "~13.2.1",
      "expo-contacts": "~12.0.1",
      "expo-file-system": "~15.2.2",
      "expo-font": "~11.1.1",
      "expo-image-picker": "~14.1.1",
      "expo-keep-awake": "~12.0.1",
      "expo-location": "~15.1.1",
      "expo-permissions": "~14.1.1",
      "expo-sensors": "~12.1.1",
      "expo-sharing": "~11.2.2",
