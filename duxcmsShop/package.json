  "scripts": {
    "build:rn-android": "duxapp runtime enterFile && taro build --type rn --platform android",
    "build:rn-ios": "duxapp runtime enterFile && taro build --type rn --platform ios",
    "android": "duxapp runtime enterFile && duxapp rn create && react-native run-android",
    "start": "duxapp runtime enterFile && taro build --type rn --watch",
    "debug:android": "duxapp runtime enterFile && duxapp rn create && cd android && ./gradlew installDebug && cd ..",
    "build:android": "duxapp runtime enterFile && duxapp rn create && cd android && ./gradlew assembleRelease && cd ..",
    "pod": "duxapp ios pod install",
    "ios": "duxapp runtime enterFile && duxapp rn create && duxapp ios pod install && react-native run-ios && cd .",
    "build:ios": "duxapp ios build",
    "export:ios": "duxapp ios export",
    "upload-ios": "duxapp ios upload dist/ios/duxapp.ipa/duxapp.ipa",
    "build:weapp": "duxapp runtime enterFile && taro build --type weapp",
    "build:swan": "duxapp runtime enterFile && taro build --type swan",
    "build:alipay": "duxapp runtime enterFile && taro build --type alipay",
    "build:tt": "duxapp runtime enterFile && taro build --type tt",
    "build:h5": "duxapp runtime enterFile && taro build --type h5",
    "build:qq": "duxapp runtime enterFile && taro build --type qq",
    "build:jd": "duxapp runtime enterFile && taro build --type jd",
    "build:quickapp": "duxapp runtime enterFile && taro build --type quickapp",
    "build:rn": "duxapp runtime enterFile && duxapp rn clearBuildAssets && taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:rn": "duxapp runtime enterFile && taro build --type rn --watch",
    "native-components:h5": "duxapp runtime enterFile && taro build native-components --type h5",
    "postinstall": "patch-package",
    "rn-create": "duxapp runtime enterFile && duxapp rn create",
    "test": "jest",
    "lint": "eslint ."
  },
