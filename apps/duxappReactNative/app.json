{
  "name": "duxappReactNative",
  "description": "react-native基础系统",
  "version": "1.0.23",
  "dependencies": [
    "duxapp"
  ],
  "npm": {
    "scripts": {
      "build:rn-android": "duxapp runtime enterFile && taro build --type rn --platform android",
      "build:rn-ios": "duxapp runtime enterFile && taro build --type rn --platform ios",
      "android": "duxapp runtime enterFile && duxapp rn create && react-native run-android && cd .",
      "start": "duxapp runtime enterFile && taro build --type rn --watch",
      "debug:android": "duxapp runtime enterFile && duxapp rn create && cd android && ./gradlew installDebug && cd ..",
      "build:android": "duxapp runtime enterFile && duxapp rn create && cd android && ./gradlew assembleRelease && cd ..",
      "pod": "duxapp ios pod install",
      "ios": "duxapp runtime enterFile && duxapp rn create && duxapp ios pod install && react-native run-ios && cd .",
      "build:ios": "duxapp ios build",
      "export:ios": "duxapp ios export",
      "upload-ios": "duxapp ios upload dist/ios/duxapp.ipa/duxapp.ipa"
    },
    "dependencies": {
      "@react-native-async-storage/async-storage": "~2.0.0",
